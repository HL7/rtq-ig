  <!-- Question 3 -->
  <item>
    <linkId value="labeling-1"/>
    <answer>
      <extension url="http://hl7.org/fhir/uv/brr/StructureDefinition/questionnaire-itemRichText">
        <valueXhtml>
          <div xmlns="http://www.w3.org/1999/xhtml">
            <p>No additional patient guidance is required. The ePI clearly states: “Store in a refrigerator (2°C – 8°C). Do not freeze.” The 36-month shelf life is supported by stability data and does not alter patient handling instructions.</p>
          </div>
        </valueXhtml>
      </extension>
    </answer>
  </item>

  <!-- Question 4 -->
  <item>
    <linkId value="labeling-2"/>
